<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddPlayer</servlet-name>
  	<display-name>AddPlayer</display-name>
  	<description></description>
  	<servlet-class>com.playermanagement.servlets.AddPlayer</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddPlayerServlet</servlet-name>
  	<display-name>AddPlayerServlet</display-name>
  	<description></description>
  	<servlet-class>com.playermanagement.servlets.AddPlayerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewPlayers</servlet-name>
  	<display-name>ViewPlayers</display-name>
  	<description></description>
  	<servlet-class>com.playermanagement.servlets.ViewPlayers</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddPlayer</servlet-name>
  	<url-pattern>/AddPlayer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddPlayerServlet</servlet-name>
  	<url-pattern>/AddPlayerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewPlayers</servlet-name>
  	<url-pattern>/ViewPlayers</url-pattern>
  </servlet-mapping>
</web-app>
